{
  "manifest_version": 2,
  "name": "Instant Downloader",
  "version": "1.1.6",
  "permissions": ["webNavigation","activeTab","tabs","*://*/*", "downloads"],
  "browser_action": {
    "default_title": "Instant Downloader"
  },
  "icons": {
    "48": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
